    "grouped_df = df.groupby('species')"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "4713a9ed-dab3-4ebf-8148-e80fad9d342a",
   "metadata": {},
   "source": [
    "<h1>Bar Plot</h1>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "6ee6d9bd-afd0-4dc6-94f0-491922e3477c",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "fig, ax = plt.subplots(figsize=(10, 6))\n",
    "\n",
    "# Define the features\n",
    "features = ['sepal_length', 'sepal_width', 'petal_length', 'petal_width']\n",
    "\n",
    "# Calculate the mean values for each feature and species\n",
    "means = [grouped_df[feature].mean().values for feature in features]\n",
    "\n",
    "# Create a clustered bar plot\n",
    "bar_width = 0.2\n",
    "x = np.arange(len(grouped_df.groups.keys()))\n",
    "\n",
    "for i, mean in enumerate(means):\n",
    "    ax.bar(x + i * bar_width, mean, bar_width, label=features[i])\n",
    "\n",
    "# Set the title and labels\n",
    "ax.set_title('Mean Features for Each Iris Species')\n",
    "ax.set_xlabel('Species')\n",
    "ax.set_ylabel('Mean Value')\n",
    "\n",
    "# Set the x-axis tick labels\n",
    "ax.set_xticks(x + 1.5 * bar_width)\n",
    "ax.set_xticklabels(grouped_df.groups.keys())\n",
    "\n",
    "# Add a legend\n",
    "ax.legend()\n",
    "\n",
    "# Show the plot\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "d2c497a4-808b-4078-bdae-ac5b3d4a5206",
   "metadata": {},
   "source": [
    "<h1>Box Plot</h1>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "05073ccf-c6b9-4775-8a2c-86c57009947c",
